{
  "name": "invoice-verification",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:type": "tsc -p tsconfig.json --noEmit",
    "test:e2e": "playwright test --reporter=list",
    "check": "npm run test:type && npm run test:e2e",
    "setup:eval": "pip install -r tools/requirements.txt",
    "seed:demo": "ts-node --transpile-only scripts/seed/items_demo.ts",
    "seed:items:csv": "ts-node --transpile-only scripts/seed/items_from_csv.ts --dir=./ --vendor=demo_vendor"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "langfuse": "^3.38.4",
    "next": "14.2.18",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.62.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/jest": "^29",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "csv-parse": "^5.5.6",
    "jest": "^29",
    "jest-environment-node": "^29",
    "pg": "^8.11.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
