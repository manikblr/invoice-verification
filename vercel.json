{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python"
    }
  },
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/validate"
    }
  ]
}