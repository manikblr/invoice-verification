{
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/validate-invoice",
      "dest": "/api/validate"
    },
    {
      "src": "/test",
      "dest": "/api/test"
    },
    {
      "src": "/debug", 
      "dest": "/api/debug"
    },
    {
      "src": "/",
      "dest": "/api/index"
    }
  ]
}